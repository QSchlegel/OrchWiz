DATABASE_URL="postgresql://user:password@localhost:5432/orchwiz?schema=public"
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
BETTER_AUTH_SECRET=
BETTER_AUTH_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000
# Comma-separated emails auto-promoted to admin role at sign-in
ORCHWIZ_ADMIN_EMAILS=
# Landing XO teaser feature controls (public-cloud kill switch)
LANDING_XO_ENABLED=true
LANDING_XO_STAGE=public-preview

# Command execution policy
ENABLE_LOCAL_COMMAND_EXECUTION=false
LOCAL_COMMAND_TIMEOUT_MS=120000
COMMAND_EXECUTION_SHELL=/bin/bash
ENABLE_LOCAL_INFRA_AUTO_INSTALL=false
LOCAL_INFRA_COMMAND_TIMEOUT_MS=600000
# Optional cloud posture gate: when true, local starship launches are blocked in Ship Yard.
CLOUD_DEPLOY_ONLY=false

# Runtime integration (OpenClaw-first with fallback)
OPENCLAW_GATEWAY_URL=
OPENCLAW_PROMPT_PATH=/v1/prompt
OPENCLAW_TIMEOUT_MS=15000
OPENCLAW_API_KEY=
OPENCLAW_DISPATCH_PATH=/v1/message
OPENCLAW_DISPATCH_TIMEOUT_MS=12000
# Optional bearer admin auth for /api/threads bridge-chat compatibility routes
BRIDGE_ADMIN_TOKEN=
# Optional bearer auth for machine Ship Yard launch/status routes
SHIPYARD_API_TOKEN=
# Optional token-auth user scope controls
SHIPYARD_API_ALLOWED_USER_IDS=
SHIPYARD_API_TOKEN_USER_ID=
SHIPYARD_API_DEFAULT_USER_ID=
SHIPYARD_API_ALLOW_IMPERSONATION=false
# Ship Yard self-healing beta lifecycle controls
SHIPYARD_SELF_HEAL_ENABLED=false
SHIPYARD_SELF_HEAL_CRON_TOKEN=
SHIPYARD_SELF_HEAL_DEFAULT_COOLDOWN_MINUTES=30
SHIPYARD_SELF_HEAL_MAX_DEPLOYMENTS_PER_RUN=20
# Local Ship Yard bootstrap can inject bridge context into these deployments
OPENCLAW_TARGET_DEPLOYMENTS=openclaw-gateway,openclaw-worker
OPENCLAW_CONTEXT_INJECTION_ENABLED=true
ENABLE_OPENAI_RUNTIME_FALLBACK=true
OPENAI_API_KEY=
OPENAI_RUNTIME_FALLBACK_MODEL=gpt-4.1-mini
CODEX_CLI_PATH=
CODEX_RUNTIME_TIMEOUT_MS=120000
CODEX_RUNTIME_MODEL=
RUNTIME_PROFILE_DEFAULT=openclaw,openai-fallback,local-fallback
RUNTIME_PROFILE_QUARTERMASTER=codex-cli,openclaw,openai-fallback,local-fallback

# Wallet enclave integration (local-only sidecar/daemon)
WALLET_ENCLAVE_ENABLED=true
WALLET_ENCLAVE_URL=http://127.0.0.1:3377
WALLET_ENCLAVE_TIMEOUT_MS=4000
WALLET_ENCLAVE_REQUIRE_BRIDGE_SIGNATURES=true
WALLET_ENCLAVE_REQUIRE_PRIVATE_MEMORY_ENCRYPTION=true
WALLET_ENCLAVE_SHARED_SECRET=

# Observability trace encryption + Langfuse transport
TRACE_ENCRYPT_ENABLED=true
TRACE_ENCRYPT_REQUIRED=true
TRACE_ENCRYPT_FIELDS=input.prompt,output.text,tool.args,tool.result,metadata.rawModelIO
LANGFUSE_BASE_URL=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
OBSERVABILITY_DECRYPT_ADMIN_TOKEN=

# Deployment connector (optional)
DEPLOYMENT_CONNECTOR_URL=
DEPLOYMENT_CONNECTOR_API_KEY=
DEPLOYMENT_AGENT_PATH=/v1/deployments/agents
DEPLOYMENT_APPLICATION_PATH=/v1/deployments/applications

# Forwarding ingest/config
ENABLE_FORWARDING_INGEST=true
FORWARDING_RATE_LIMIT=60
FORWARDING_RATE_WINDOW_MS=60000
DEFAULT_FORWARDING_API_KEY=orchwiz-dev-forwarding-key
DEFAULT_SOURCE_NODE_ID=local-node
DEFAULT_SOURCE_NODE_NAME=Local Node
FORWARD_TARGET_URL=
FORWARD_API_KEY=
FORWARDING_FEATURE_ENABLED=true
FORWARDING_TEST_TARGET_ALLOWLIST=localhost,127.0.0.1

# Bridge connection dispatch queue
BRIDGE_DISPATCH_RETRY_BASE_MS=1000
BRIDGE_DISPATCH_MAX_ATTEMPTS=6
BRIDGE_DISPATCH_RETAIN_COUNT=500

# GitHub webhook behavior
GITHUB_WEBHOOK_SECRET=
ENABLE_GITHUB_WEBHOOK_COMMENTS=false
GITHUB_TOKEN=

# SSE stream toggle (client-side behavior can check this flag)
ENABLE_SSE_EVENTS=true
STRICT_RESOURCE_OWNERSHIP=true

# AgentSync heuristic RL loop
AGENTSYNC_ENABLED=true
AGENTSYNC_CRON_TOKEN=
AGENTSYNC_LOOKBACK_DAYS=14
AGENTSYNC_MIN_SIGNALS=3

# Security audits
SECURITY_AUDIT_CRON_TOKEN=
ENABLE_BRIDGE_CREW_LIVE_STRESS=false

# UI context window reference for relative usage indicators
NEXT_PUBLIC_CONTEXT_WINDOW_TOKENS=258000

# Vault read/search/graph limits
VAULT_MAX_PREVIEW_BYTES=262144
VAULT_MAX_EDIT_BYTES=2097152
VAULT_SEARCH_MAX_BYTES=131072
VAULT_GRAPH_MAX_NOTES=2000
VAULT_GRAPH_MAX_EDGES=10000
VAULT_RAG_ENABLED=true
VAULT_RAG_EMBEDDING_MODEL=text-embedding-3-small
VAULT_RAG_TOP_K=12
VAULT_RAG_SYNC_ON_WRITE=true
VAULT_RAG_CHUNK_CHARS=900
VAULT_RAG_MAX_CHUNKS_PER_DOC=120
VAULT_RAG_EMBED_BATCH_SIZE=24
VAULT_RAG_QUERY_CANDIDATE_LIMIT=1800

# Meshy 3D API (bridge agent character models). Get key from https://www.meshy.ai/settings/api
MESHY_API_KEY=
